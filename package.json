{
  "name": "components-router",
  "version": "0.4.6",
  "description": "Vue Router like React-Router",
  "keywords": [
    "vue",
    "router",
    "components"
  ],
  "author": "scioli0315",
  "license": "MIT",
  "main": "lib/components-router.cjs.js",
  "module": "lib/components-router.esm-bundler.js",
  "homepage": "https://github.com/scioli0315/components-router#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scioli0315/components-router.git"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "playground:dev": "vite --config playground/vite.config.ts --port 8888",
    "playground:build": "vue-tsc --noEmit && vite build --config playground/vite.config.ts",
    "docs:dev": "vitepress dev docs --port 8889",
    "docs:build": "vitepress build docs",
    "pub": "npm publish --registry=https://registry.npmjs.com --access=public",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext vue"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/**/*.d.ts",
    "vetur/tags.json"
  ],
  "types": "lib/index.d.ts",
  "vetur": {
    "tags": "vetur/tags.json"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "history": "^5.0.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "core-js": "^3.16.1",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^7.16.0",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.1",
    "rollup": "^2.56.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.5",
    "upath": "^2.0.1",
    "vite": "^2.5.0",
    "vitepress": "^0.16.1",
    "vue": "^3.2.4",
    "vue-tsc": "^0.2.3"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run lint",
      "git add"
    ]
  }
}
